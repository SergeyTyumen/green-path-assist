    {
      "parameters": {
        "jsCode": "const functionName = $('Extract Function Call').first().json.function_name;\nconst apiResponse = $json;\n\nlet formattedResponse = '';\n\nswitch (functionName) {\n  case 'create_task':\n    if (apiResponse && apiResponse.length > 0) {\n      const task = apiResponse[0];\n      formattedResponse = `Задача \"${task.title}\" успешно создана с приоритетом ${task.priority}.`;\n    } else {\n      formattedResponse = 'Задача создана успешно.';\n    }\n    break;\n\n  case 'get_tasks':\n    if (apiResponse && apiResponse.length > 0) {\n      const tasks = apiResponse;\n      formattedResponse = `Найдено ${tasks.length} задач:\\n`;\n      tasks.forEach((task, index) => {\n        formattedResponse += `${index + 1}. ${task.title} (${task.status})\\n`;\n      });\n    } else {\n      formattedResponse = 'Задач не найдено.';\n    }\n    break;\n\n  case 'get_clients':\n    if (apiResponse && apiResponse.length > 0) {\n      const clients = apiResponse;\n      formattedResponse = `Найдено ${clients.length} клиентов:\\n`;\n      clients.forEach((client, index) => {\n        formattedResponse += `${index + 1}. ${client.name} - ${client.phone} (${client.status})\\n`;\n      });\n    } else {\n      formattedResponse = 'Клиентов не найдено.';\n    }\n    break;\n\n  case 'get_leads_stats':\n    if (apiResponse && typeof apiResponse === 'object') {\n      formattedResponse = apiResponse.response || JSON.stringify(apiResponse, null, 2);\n    } else {\n      formattedResponse = 'Данные получены успешно.';\n    }\n    break;\n\n  default:\n    formattedResponse = 'Операция выполнена успешно.';\n}\n\nreturn {\n  json: {\n    response: formattedResponse,\n    success: true,\n    timestamp: new Date().toISOString()\n  }\n};"
      },
      "id": "format-function-response",
      "name": "Format Function Response",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1780, 300]
    },
    {
      "parameters": {
        "jsCode": "const textResponse = $('Extract Function Call').first().json.text_response;\n\nreturn {\n  json: {\n    response: textResponse || 'Извините, не смог обработать ваш запрос.',\n    success: true,\n    timestamp: new Date().toISOString()\n  }\n};"
      },
      "id": "format-text-response",
      "name": "Format Text Response",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1120, 400]
    },
    {
      "parameters": {
        "respondWith": "json",
        "responseBody": "={{ {\n  response: $json.response,\n  success: $json.success,\n  timestamp: $json.timestamp\n} }}"
      },
