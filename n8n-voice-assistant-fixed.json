      },
      "id": "get-leads-stats",
      "name": "Get Leads Stats",
      "type": "n8n-nodes-base.httpRequest",
      "typeVersion": 4.2,
      "position": [1560, 260],
      "credentials": {
        "supabaseApi": {
          "id": "supabase-credentials",
