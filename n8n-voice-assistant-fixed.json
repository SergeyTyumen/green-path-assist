        "sendBody": true,
        "contentType": "json",
        "jsonBody": "={{ {\n  title: $json.function_args.title,\n  description: $json.function_args.description,\n  priority: $json.function_args.priority || 'medium',\n  status: 'pending',\n  user_id: $json.user_id\n} }}"
      },
