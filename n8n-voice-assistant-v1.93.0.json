    {
      "parameters": {
        "jsCode": "// Форматируем ответ от Supabase API\nconst functionName = $('Extract Function Call').first().json.function_name;\nconst apiResponse = $json;\n\nlet formattedResponse = '';\n\nswitch (functionName) {\n  case 'create_task':\n    if (apiResponse && apiResponse.length > 0) {\n      const task = apiResponse[0];\n      formattedResponse = `Задача \"${task.title}\" успешно создана с приоритетом ${task.priority}.`;\n    } else {\n      formattedResponse = 'Задача создана успешно.';\n    }\n    break;\n\n  case 'get_tasks':\n    if (apiResponse && apiResponse.length > 0) {\n      const tasks = apiResponse;\n      formattedResponse = `Найдено ${tasks.length} задач:\\n`;\n      tasks.forEach((task, index) => {\n        formattedResponse += `${index + 1}. ${task.title} (${task.status})\\n`;\n      });\n    } else {\n      formattedResponse = 'Задач не найдено.';\n    }\n    break;\n\n  case 'get_clients':\n    if (apiResponse && apiResponse.length > 0) {\n      const clients = apiResponse;\n      formattedResponse = `Найдено ${clients.length} клиентов:\\n`;\n      clients.forEach((client, index) => {\n        formattedResponse += `${index + 1}. ${client.name} - ${client.phone} (${client.status})\\n`;\n      });\n    } else {\n      formattedResponse = 'Клиентов не найдено.';\n    }\n    break;\n\n  case 'create_client':\n    if (apiResponse && apiResponse.length > 0) {\n      const client = apiResponse[0];\n      formattedResponse = `Клиент \"${client.name}\" успешно добавлен в систему.`;\n    } else {\n      formattedResponse = 'Клиент создан успешно.';\n    }\n    break;\n\n  case 'find_client':\n    if (apiResponse && apiResponse.length > 0) {\n      const clients = apiResponse;\n      formattedResponse = `Найдено ${clients.length} клиентов:\\n`;\n      clients.forEach((client, index) => {\n        formattedResponse += `${index + 1}. ${client.name} - ${client.phone}\\n`;\n      });\n    } else {\n      formattedResponse = 'Клиенты не найдены.';\n    }\n    break;\n\n  case 'get_leads_stats':\n  case 'create_estimate':\n  case 'get_analytics':\n    if (apiResponse && typeof apiResponse === 'object') {\n      formattedResponse = apiResponse.response || JSON.stringify(apiResponse, null, 2);\n    } else {\n      formattedResponse = 'Данные получены успешно.';\n    }\n    break;\n\n  default:\n    formattedResponse = 'Операция выполнена успешно.';\n}\n\nreturn {\n  json: {\n    response: formattedResponse,\n    success: true,\n    timestamp: new Date().toISOString(),\n    function_executed: functionName\n  }\n};"
      },
