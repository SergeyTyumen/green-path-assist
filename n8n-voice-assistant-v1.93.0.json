      "parameters": {
        "jsCode": "// Форматируем ответ от функции в читаемый текст\nconst functionResponse = $json;\nconst functionName = $('Extract Function Call').first().json.function_name;\n\nlet responseText = '';\n\ntry {\n  switch(functionName) {\n    case 'create_task':\n      if (functionResponse.id) {\n        responseText = `Задача \"${functionResponse.title}\" успешно создана. Приоритет: ${functionResponse.priority}, статус: ${functionResponse.status}.`;\n      } else {\n        responseText = 'Не удалось создать задачу. Попробуйте ещё раз.';\n      }\n      break;\n      \n    case 'get_tasks':\n      if (Array.isArray(functionResponse) && functionResponse.length > 0) {\n        const tasksList = functionResponse.slice(0, 5).map(task => \n          `- ${task.title} (${task.status}, приоритет: ${task.priority})`\n        ).join('\\n');\n        responseText = `Найдено задач: ${functionResponse.length}\\n\\nПоследние задачи:\\n${tasksList}`;\n      } else {\n        responseText = 'Задачи не найдены.';\n      }\n      break;\n      \n    case 'get_clients':\n      if (Array.isArray(functionResponse) && functionResponse.length > 0) {\n        const clientsList = functionResponse.slice(0, 5).map(client => \n          `- ${client.name} (${client.status})${client.phone ? ', тел: ' + client.phone : ''}`\n        ).join('\\n');\n        responseText = `Найдено клиентов: ${functionResponse.length}\\n\\nПоследние клиенты:\\n${clientsList}`;\n      } else {\n        responseText = 'Клиенты не найдены.';\n      }\n      break;\n      \n    case 'create_client':\n      if (functionResponse.id) {\n        responseText = `Клиент \"${functionResponse.name}\" успешно добавлен в CRM.`;\n      } else {\n        responseText = 'Не удалось создать клиента. Проверьте данные.';\n      }\n      break;\n      \n    case 'find_client':\n      if (Array.isArray(functionResponse) && functionResponse.length > 0) {\n        const clientsList = functionResponse.map(client => \n          `- ${client.name}${client.phone ? ' (' + client.phone + ')' : ''} - ${client.status}`\n        ).join('\\n');\n        responseText = `Найдено клиентов: ${functionResponse.length}\\n${clientsList}`;\n      } else {\n        responseText = 'Клиенты не найдены по вашему запросу.';\n      }\n      break;\n      \n    case 'create_estimate':\n      if (functionResponse.success || functionResponse.response) {\n        responseText = functionResponse.response || 'Смета успешно создана.';\n      } else {\n        responseText = 'Не удалось создать смету. Попробуйте ещё раз.';\n      }\n      break;\n      \n    case 'get_leads_stats':\n    case 'get_analytics':\n      if (functionResponse.success && functionResponse.response) {\n        responseText = functionResponse.response;\n      } else if (typeof functionResponse === 'object') {\n        responseText = 'Данные получены, но требуют дополнительной обработки.';\n      } else {\n        responseText = 'Не удалось получить аналитические данные.';\n      }\n      break;\n      \n    default:\n      responseText = 'Функция выполнена, но формат ответа не распознан.';\n  }\n} catch (error) {\n  responseText = `Ошибка при обработке ответа: ${error.message}`;\n}\n\nreturn {\n  json: {\n    response: responseText,\n    function_name: functionName,\n    raw_response: functionResponse,\n    timestamp: new Date().toISOString(),\n    success: true\n  }\n};"
      },
      "id": "format-function-response",
      "name": "Format Function Response",
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [1780, 300]
    },
    {
      "parameters": {
        "jsCode": "// Обрабатываем текстовый ответ от OpenAI\nconst textResponse = $json.text_response || 'Извините, не смог обработать ваш запрос.';\n\nreturn {\n  json: {\n    response: textResponse,\n    function_name: null,\n    raw_response: $json.original_response,\n    timestamp: new Date().toISOString(),\n    success: true\n  }\n};"
      },
